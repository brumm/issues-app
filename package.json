    "github-parse-link": "^1.1.1",
    "github-request-all": "^1.1.0",
    "halogen": "^0.2.0",
    "idb-keyval": "^2.3.0",
    "markdown-it": "^8.3.1",
    "markdown-it-emoji": "^1.3.0",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-task-lists": "^1.4.1",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.3",
    "normalizr": "^3.2.2",
    "parse-github-url": "^1.0.0",
    "qry": "^0.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-octicon": "^2.0.0",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0-beta.6",
    "react-timeago": "^3.2.0",
