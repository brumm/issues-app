{
  "name": "issues",
  "productName": "Issues",
  "version": "1.0.0",
  "description": "Because you got issues",
  "main": "dist/index.js",
  "private": true,
  "prettier": {
    "print-width": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": false,
    "bracketSpacing": true
  },
  "scripts": {
    "build": "nwb build && babel main -d dist && npm run package",
    "watch-electron-main": "babel main -d dist -w",
    "clean": "nwb clean",
    "start": "babel main -d dist && npm run server",
    "server": "node dev-server.js --run-electron",
    "test": "nwb test",
    "test:watch": "nwb test --server",
    "package": "rm -rf ./release && electron-packager --icon icon-1024.icns --overwrite --asar --ignore='src|main|tests' --prune --platform=darwin --arch=x64 --out='release' ."
  },
  "dependencies": {
    "electron-devtools-installer": "^2.2.3",
    "electron-irpc": "0.0.2",
    "electron-window-state": "^4.1.1",
    "idb-keyval": "^3.0.1",
    "lodash.isplainobject": "^4.0.6",
    "markdown-it": "^8.4.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "normalizr": "^3.2.4",
    "parse-github-url": "^1.0.2",
    "parse-link-header": "^1.0.1",
    "qry": "^0.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-octicon": "^3.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-svg-spinner": "^1.0.0",
    "react-timeago": "^3.4.3",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "redux-batched-actions": "^0.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "remove-markdown": "^0.2.2",
    "striptags": "^3.1.1",
    "tinycolor2": "^1.4.1",
    "uuid": "^3.2.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "electron": "^1.8.4",
    "electron-packager": "^11.1.0",
    "express": "^4.16.3",
    "minimist": "^1.2.0",
    "nwb": "^0.21.5",
    "nwb-sass": "^0.8.3"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
